<?xml version="1.0" encoding="utf-8" ?> 
<mapping>
	<class name="tufts.vue.MapItem">
		<map-to xml="map-item"/>
		<field name="ID" type="java.lang.String">
			<bind-xml name="ID" node="attribute"/>
		</field>
		<field name="label" type="java.lang.String">
			<bind-xml name="label" node="attribute"/>
		</field>
		<field name="notes" type="java.lang.String">
			<bind-xml name="notes" node="element"/>
		</field>
		<field name="metaData" type="java.lang.String">
			<bind-xml name="metaData" node="element"/>
		</field>
		<field name="category" type="java.lang.String">
			<bind-xml name="category" node="element"/>
		</field>
		<field name="resource" type="tufts.vue.Resource">
			<bind-xml name="resource" node="element"/>
		</field>
		<field name="x" type="float">
			<bind-xml name="x" node="attribute"/>
		</field>
		<field name="y" type="float">
			<bind-xml name="y" node="attribute"/>
		</field>
	</class>
	
	<class name="tufts.vue.Node" extends="tufts.vue.MapItem">
		<map-to xml="node"/>
	</class>
	
	<class name="tufts.vue.Link" extends="tufts.vue.MapItem">
		<map-to xml="link"/>
		<field name="item1_ID" type="java.lang.String">
			<bind-xml name="item1_ID" node="attribute"/>
		</field>
	        <field name="item2_ID" type="java.lang.String">
			<bind-xml name="item2_ID" node="attribute"/>
		</field>
	</class>
	
	<class name="tufts.vue.ConceptMap" extends="tufts.vue.MapItem">
		<map-to xml="concept-map"/>
		<field name="nodeList" type="tufts.vue.Node" collection="vector">
			<bind-xml name="node"/>
		</field>
		<field name="linkList" type="tufts.vue.Link" collection="vector">
			<bind-xml name="link"/>
		</field>
	</class>
</mapping>
